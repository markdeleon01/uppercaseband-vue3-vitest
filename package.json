{
  "name": "uppercaseband-vue3-vitest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit",
    "test:unit": "vitest --run --passWithNoTests --environment jsdom --root src/",
    "cy:e2e": "start-server-and-test preview :4173 'cypress run --e2e --record --key bdb18446-87ba-435b-91a1-45c4ce059f7e'",
    "cy:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "coverage": "vitest run --coverage --environment jsdom",
    "install:xvfb": "sudo apt-get update && sudo apt-get install libgtk2.0-0 && sudo apt-get install libgtk-3-0 && sudo apt-get install libgbm-dev && sudo apt-get install libnotify-dev && sudo apt-get install libgconf-2-4 && sudo apt-get install libnss3 && sudo apt-get install libxss1 && sudo apt-get install libasound2 && sudo apt-get install libxtst6 && sudo apt-get install xauth && sudo apt-get install xvfb",
    "make-badges": "npx istanbul-badges-readme",
    "make-badges:ci": "npm run make-badges -- --ci",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cypress": "^13.2.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.4.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/coverage-istanbul": "^0.34.5",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.50.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "istanbul-badges-readme": "^1.8.5",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "start-server-and-test": "^2.0.1",
    "vite": "^4.4.5",
    "vitest": "^0.34.5"
  }
}
